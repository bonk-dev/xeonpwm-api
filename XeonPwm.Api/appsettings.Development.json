{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "XeonPwm.*": "Trace",
      "Microsoft.AspNetCore.SignalR.*": "Debug"
    }
  },
  "Cors": {
    "Enable": true,
    "Origins": [
      "http://localhost:3000",
      "sdadasdas"
    ]
  },
  "Sensors": {
    "OutputLogPath": "/srv/pwm/sensors.log"
  },
  "ConnectionStrings": {
    "Sqlite": "Data Source=/tmp/xeonpwm.db;"
  },
  "Driver": {
    "SerialPort": "/tmp/interceptty",
    "BaudRate": 115200
  },
  "AutoMode": {
    "AutoEnable": true
  },
  "Authentication": {
    "HashIteration": 600000
  }
}
